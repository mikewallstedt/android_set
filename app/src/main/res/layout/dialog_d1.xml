<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center" >
    
    <LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal" >
		    <ImageView
		        android:id="@+id/d1gf"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:src="@drawable/d1gf"
		        android:contentDescription="@string/example_card_description" />
		    <ImageView
		        android:id="@+id/d1gh"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:src="@drawable/d1gh"
		        android:contentDescription="@string/example_card_description" />
		    <ImageView
		        android:id="@+id/d1ge"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:src="@drawable/d1ge"
		        android:contentDescription="@string/example_card_description" />
		</LinearLayout>
		
	    <LinearLayout 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">
	        <ImageView
		        android:id="@+id/d1rf"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:src="@drawable/d1rf"
		        android:contentDescription="@string/example_card_description" />
		    <ImageView
		        android:id="@+id/d1rh"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:src="@drawable/d1rh"
		        android:contentDescription="@string/example_card_description" />
			<ImageView
			    android:id="@+id/d1re"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:src="@drawable/d1re"
		        android:contentDescription="@string/example_card_description" />
	    </LinearLayout>
	    
	    <LinearLayout 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">
			<ImageView
			    android:id="@+id/d1pf"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:src="@drawable/d1pf"
		        android:contentDescription="@string/example_card_description" />
			<ImageView
			    android:id="@+id/d1ph"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:src="@drawable/d1ph"
		        android:contentDescription="@string/example_card_description" />
			<ImageView
			    android:id="@+id/d1pe"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:src="@drawable/d1pe"
		        android:contentDescription="@string/example_card_description" />
		</LinearLayout>
    

</LinearLayout>
